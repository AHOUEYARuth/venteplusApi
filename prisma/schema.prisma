generator client {
    provider = "prisma-client-js"
}


datasource db {
    provider = "mongodb"
    url = env("DATABASE_URL")
}


model Sellers {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    phoneNumber String @unique
    email String?
    password String
    name String?
    avatarUrl String? 
    createdAt DateTime @default(now())
}